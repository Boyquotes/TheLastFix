[application]

config/name="Plumber Game"
run/main_scene="res://scenes/main_menu/main_menu.tscn"
config/icon="res://icon.png"

[autoload]

Game="*res://main.tscn"

[display]

window/size/width=256
window/size/height=144
window/size/test_width=1024
window/size/test_height=576
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[editor_plugins]

enabled=PoolStringArray( "res://addons/vnen.tiled_importer/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[importer_defaults]

vnen.tiled_importer={
"collision_layer": 2,
"image_flags": 3,
"post_import_script": "res://tiled_import.gd"
}
vnen.tiled_tileset_importer={
"image_flags": 3
}
texture={
"flags/filter": false
}

